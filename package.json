{
  "name": "dummy-array",
  "version": "0.0.1",
  "description": "Very simple package for creating a dummy array",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whoyoux/dummy-array.git"
  },
  "keywords": [
    "dummy-array",
    "array",
    "dummy"
  ],
  "author": "whoyoux",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/whoyoux/dummy-array/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/whoyoux/dummy-array#readme",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.5"
  }
}
